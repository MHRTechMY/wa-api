    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "peerDependencies": {
    "crypto-js": "*",
    "mongoose": "*",
    "redis": "*"
  },
  "packageManager": "yarn@4.9.2"
}
